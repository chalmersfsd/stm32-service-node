char *payload = NULL;
  		cats(&payload, STATUS);
  		cats(&payload, DELIMITER);
  		cats(&payload, EBS_LINE);
  		cats(&payload, DELIMITER);
  		cats(&payload, rawString);
  		
			int payloadLength = strlen(payload);
			cats(&payload, MSG_END); 
			char* netstringMsg = NULL;

			itoa(payloadLength, netstringMsg, 10);
			cats(netstringMsg, ":");
			cats(netstringMsg, payload);
			cats(netstringMsg, MSG_END);
			
			int bytesToWrite = strlen(netstringMsg);
  		int bytesToWriteLeft = bytesToWrite;
  		int bytesWritten = 0;
  		
  		while(bytesToWriteLeft > 0){
  		  blinkLedBlue = true;
  			bytesWritten = sdWriteTimeout(&SDU1, (uint8_t*)netstringMsg, bytesToWrite, timeOut);
  			bytesToWriteLeft -= bytesWritten;
  		}
  		//free buffers
  		cats(&payload,NULL);
  		cats(&netstringMsg,NULL);
  		bytesToWrite = 0;
